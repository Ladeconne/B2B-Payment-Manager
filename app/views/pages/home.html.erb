<div id="home">
  <div class="section-title">
    <h1>Welcome to Beatle</h1>
    <h3>Manager your finances in a simple and understandable way</h3>
  </div>
  <div class="section-devise">
    <div class="devise-content">
      <p>If you already have an account please log in <br>
      otherwise click on the sign in button</p>
      <div class="button-wrapper">
        <%= link_to  "log-in", new_user_session_path, class:"button button-outlined" %>
        <%= link_to  "sign-up",  new_user_registration_path, class:"button" %>
      </div>
    </div>
  </div>
</div>
